{
  "name": "contentful-schema-diff",
  "version": "0.7.2",
  "description": "",
  "main": "dist/main.js",
  "bin": {
    "contentful-schema-diff": "bin/index.js"
  },
  "scripts": {
    "lint": "tslint 'src/**/*.ts'",
    "test": "tsc --project tsconfig.test.json && nyc ava 'src/**/*.test.js'",
    "coverage": "npm run test && nyc report --reporter=html && open coverage/index.html",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "test-watch": "ava --watch",
    "build": "tsc",
    "prepare": "npm run build"
  },
  "author": "Gordon Burgett (gordon@gordonburgett.net)",
  "license": "MIT",
  "repository": "https://github.com/watermarkchurch/contentful-schema-diff",
  "devDependencies": {
    "@types/fs-extra": "^5.0.2",
    "@types/lodash": "^4.14.116",
    "@types/node": "^9.6.4",
    "@types/yargs": "^11.0.0",
    "ava": "1.0.0-rc.1",
    "coveralls": "^3.0.2",
    "nyc": "^13.1.0",
    "ts-node": "^7.0.1",
    "tslint-eslint-rules": "^5.4.0"
  },
  "dependencies": {
    "contentful-management": "^5.0.0-beta2",
    "fs-extra": "^5.0.0",
    "json-diff": "^0.5.2",
    "lodash": "^4.17.10",
    "tslint": "^5.11.0",
    "typescript": "^3.1",
    "typescript-formatter": "^7.2.2",
    "yargs": "^11.0.0"
  },
  "ava": {
    "files": [
      "src/**/*.test.ts"
    ],
    "cache": true,
    "failFast": true,
    "verbose": true,
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  }
}
