          "required": true,
          "validations": [
            {
              "size": {
                "min": 1,
                "max": 60
              },
              "message": "A Menu Button should have a very short text field - ideally a single word.  Please limit the text to 60 characters."
            }
          ],
          "disabled": false,
          "omitted": false
